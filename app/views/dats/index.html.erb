<%if params[:id] == 'overview'%>
<%=render 'overview', {:students =>@students}%>
<%end%>
<%if params[:id] == 'individual'%>
<%=render 'individuals', {:students =>@students}%>
<%end%>
<%if params[:id] == 'subject'%>
<%=render 'subject', {:students =>@students}%>
<%end%>
<%if params[:id] == 'both'%>
	
	<%=render 'subject', {:students =>@students}%>
	<%=render 'overview', {:students =>@students}%>
<%end%>
<%if params[:id] == 'front'%>
	<%=render 'front_page'%>
	<%=render 'content_description'%>
<%end%>
<%if params[:id] == 'contents'%>
	<div class="cont">
		<%=render 'contents'%>
	</div>
<%end%>

<%if params[:id] == 'subject_page'%>
	<div class="cont">
		<%=render 'subject_page', {:subject => "AQA Maths"}%>
	</div>
<%end%>
