<div class="page">
		<p class="qual_title">Subject Overview</p>
		
		<table class="table">
			<tr>
				<th>Qualification</th>
				<th>APS</th>
				<th>LP</th>
				<th>VA</th>
				
			</tr>
		<%data.each do |s|%>
			<tr>
				<td><%=s[:name]%></td>
				<td><%=s[:ps]%></td>
				<td><%=s[:lp]%></td>
				<td><%=s[:va]%></td>
			<tr>
		<%end%>
		</table>
	</div>
	<div class="page">
		<p class="qual_title">Subject Overview</p>
		<p class="class_title">Levels of Progress by Subject</p>
		<div style="width: 900px; height: 600px;" id='lp_overview_chart'></div>	
		<p class="class_title">Value Added Score by Subject</p>
		<div style="width: 900px; height: 600px;" id='va_overview_chart'></div>	
	</div>
	<div class="page">
		<p class="qual_title">Subject Overview</p>
		<p class="class_title">Average Point Score by Subject</p>
		<div style="width: 900px; height: 600px;" id='ps_overview_chart'></div>	
	</div>
	<div class="page">
		<p class="qual_title">Subject Overview - A* - C</p>
		<table class="table">
			<tr>
				<th>Qualification</th>
				<th>A* - G (%)</th>
				<th>A* - C (%)</th>
				<th>A* - A (%)</th>
			</tr>
		<%data.each do |s|%>
			<tr>
				<td><%=s[:name]%></td>
				<td>
				<%if s[:a_g]<95%>
					<div class="numberCircle_percent below_color">
				<%elsif s[:a_g]<99%>
					<div class="numberCircle_percent one_below_color">
				<%elsif s[:a_g]<100%>
					<div class="numberCircle_percent on_track_color">
				<%elsif s[:a_g]=100%>
					<div class="numberCircle_percent above_color">
				<%end%>
				<%=s[:a_g]%></div></td>
				<td>
				<%if s[:a_c]<50%>
					<div class="numberCircle_percent below_color">
				<%elsif s[:a_c]<65%>
					<div class="numberCircle_percent one_below_color">
				<%elsif s[:a_c]<75%>
					<div class="numberCircle_percent on_track_color">
				<%elsif s[:a_c]>=75%>
					<div class="numberCircle_percent above_color">
				<%end%>
				<%=s[:a_c]%></div></td>
				<td>
				<%if s[:a_a]<10%>
					<div class="numberCircle_percent below_color">
				<%elsif s[:a_a]<35%>
					<div class="numberCircle_percent one_below_color">
				<%elsif s[:a_a]<40%>
					<div class="numberCircle_percent on_track_color">
				<%elsif s[:a_a]>=40%>
					<div class="numberCircle_percent above_color">
				<%end%>
				<%=s[:a_a]%></div></td>
			</tr>
		<%end%>
	</div>
	
	
    
    <script type="text/javascript">
      google.load("visualization", "1", {packages:["corechart"]});
      google.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Subject', 'Lp',  { role: 'style' } ],
		<%data.each do |d|%>
			<%if d[:lp] <0%>
				['<%=d[:name]%>',  <%=d[:lp]%>,'#000000'],
			<%elsif d[:lp] <3%>
         	 	['<%=d[:name]%>',  <%=d[:lp]%>,'#d9534f'],
         	<%elsif d[:lp] <4%>
         		['<%=d[:name]%>',  <%=d[:lp]%>,'#f0ad4e'],
         	<%elsif d[:lp] <5 %>
         		['<%=d[:name]%>',  <%=d[:lp]%>,'#5cb85c'],
         	<%elsif d[:lp] >=5%>
         		['<%=d[:name]%>',  <%=d[:lp]%>,'#428bca'],
         	<%end%>
		<%end%>
        ]);

        var options = {
          title: '',
          vAxis: {title: "",  titleTextStyle: {color: 'red'}, textStyle: {fontSize: 12}},
          chartArea: {  width: "50%", height: "90%", left:450, top:0 },
           legend: 'none',
          hAxis: {title: 'Levels of Progress',  titleTextStyle: {color: 'grey'}}
        };

        var chart = new google.visualization.BarChart(document.getElementById('lp_overview_chart'));

        chart.draw(data, options);
      }
   
    </script>
    
    <script type="text/javascript">
      google.load("visualization", "1", {packages:["corechart"]});
      google.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
