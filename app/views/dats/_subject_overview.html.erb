         	 	['<%=d[:name]%>',  <%=d[:va]%>,'#d9534f'],
         	<%elsif d[:va] <1%>
         		['<%=d[:name]%>',  <%=d[:va]%>,'#f0ad4e'],
         	<%elsif d[:va] <2 %>
         		['<%=d[:name]%>',  <%=d[:va]%>,'#5cb85c'],
         	<%elsif d[:va] >=2%>
         		['<%=d[:name]%>',  <%=d[:va]%>,'#428bca'],
         	<%end%>
		<%end%>
        ]);

        var options = {
          title: '',
          vAxis: {title: "",  titleTextStyle: {color: 'red'}, textStyle: {fontSize: 12}},
          chartArea: {  width: "50%", height: "90%", left:450, top:0 },
           legend: 'none',
          hAxis: {title: 'Value Added',  titleTextStyle: {color: 'grey'}}
        };

        var chart = new google.visualization.BarChart(document.getElementById('va_overview_chart'));

        chart.draw(data, options);
      }
   
    </script>
    
